<h2><%= @house.name %> <%= link_to "(edit)", edit_house_path(@house) %></h2>
<h4><%= @house.sigil %></h4>

<h3>Characters <%= link_to "(+)", new_house_character_path(@house) %></h3>

<ul>
  <% @house.characters.each do |character| %>
    <li><%= link_to "#{character.character_name}", character %></li>
  <% end %>
</ul>